  PACKAGE_NAME      = RedfishPkg
  PACKAGE_GUID      = c432b76e-5232-11e7-9010-005056c00008
  PACKAGE_VERSION   = 0.1

[Includes]
  Include

[Includes.Common.Private]
  Library/LibredfishLib/libredfish/bindings/uefi/include
  Library/JanssonLib/jansson/src
  Library/Include

[LibraryClasses]
  BaseJsonLib|Include/Library/BaseJsonLib.h
  RedfishLib|Include/Library/RedfishLib.h

[Protocols]
  ## Include/Protocol/RedfishCredential.h
