[Guids]
  gEfiRedfishPkgTokenSpaceGuid      = { 0x4fdbccb7, 0xe829, 0x4b4c, { 0x88, 0x87, 0xb2, 0x3f, 0xd7, 0x25, 0x4b, 0x85 }}

[PcdsFixedAtBuild, PcdsPatchableInModule]
  #
  # This PCD is the UEFI device path of network device which should be used as the Redfish host interface.
  #
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishRestExServiceDevicePath|{0x0}|REST_EX_SERVICE_DEVICE_PATH_DATA|0x00001003 {
  <HeaderFiles>
    Pcd/RestExServiceDevicePath.h
  <Packages>
    MdePkg/MdePkg.dec
    MdeModulePkg/MdeModulePkg.dec
    RedfishPkg/RedfishPkg.dec
  }
  #
  # This PCD indicates the EFI REST EX access mode to Redfish service.
  # Default is set to out of band access.
  #
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishRestExServiceAccessModeInBand|FALSE|BOOLEAN|0x00001004
  #
  # This PCD indicates the access mode EFI Discover protocol uses to look for the proper EFI REST EX
  # protocol instance.
  #
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishDiscoverAccessModeInBand|FALSE|BOOLEAN|0x00001005
  #
  # String PCDs used for build up default BIOS Attribute Registry properties.
  #
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishAttributeRegistryOdataType|"#AttributeRegistry.v1_0_0.AttributeRegistry"|VOID*|0x00001100
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishAttributeRegistryOdataId|"/redfish/v1/Registries/BiosAttributeRegistryUefiKeyword.v1_0_0"|VOID*|0x00001101
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishAttributeRegistryDescription|"This registry defines a representation of BIOS Attribute instances (UEFI configuration keyword)"|VOID*|0x00001102
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishAttributeRegistryId|"BiosAttributeRegistryUefiKeyword.v1_0_0"|VOID*|0x00001103
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishAttributeRegistryLanguage|"en"|VOID*|0x00001104
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishAttributeRegistryName|"BIOS Attribute Registry"|VOID*|0x00001105
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishAttributeRegistryOwningEntity|"EDKII"|VOID*|0x00001106
  gEfiRedfishPkgTokenSpaceGuid.PcdRedfishAttributeRegistryVersion|"1.0.0"|VOID*|0x00001107
